{
    "name": "modular-model-spec",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "vite dev --port 5176",
        "build": "vite build",
        "preview": "vite preview",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "sync": "surreal import schema.surql --endpoint http://127.0.0.1:8000 -u root -p root --ns development --db modular_model_spec",
        "sync:queries": "surreal-codegen --dir './surql/' --schema './schema.surql' --output './src/lib/queries.ts' --header 'import { type RecordId, Surreal } from \"$lib/pojo_surreal\"'",
        "lint": "eslint ."
    },
    "devDependencies": {
        "@sveltejs/adapter-auto": "^3.3.1",
        "@sveltejs/kit": "^2.9.0",
        "@sveltejs/vite-plugin-svelte": "^5.0.1",
        "@types/eslint": "^8.56.12",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^9.16.0",
        "eslint-plugin-svelte": "^2.46.1",
        "svelte": "^5.8.1",
        "svelte-check": "^4.1.1",
        "tslib": "^2.8.1",
        "typescript": "^5.7.2",
        "vite": "^6.0.3"
    },
    "type": "module",
    "dependencies": {
        "@auth/core": "^0.37.4",
        "@auth/sveltekit": "^1.7.4",
        "@resvg/resvg-js": "^2.6.2",
        "@sveltejs/adapter-vercel": "^5.5.0",
        "@types/mdast": "^4.0.4",
        "acorn": "^8.14.0",
        "highlight.js": "^11.10.0",
        "mdast": "^3.0.0",
        "mdast-util-directive": "^3.0.0",
        "mdast-util-from-markdown": "^2.0.2",
        "mdast-util-gfm-strikethrough": "^2.0.0",
        "mdast-util-gfm-table": "^2.0.0",
        "mdast-util-mdx": "^3.0.0",
        "mdast-util-mdx-jsx": "^3.1.3",
        "micromark-extension-directive": "^3.0.2",
        "micromark-extension-gfm-strikethrough": "^2.1.0",
        "micromark-extension-gfm-table": "^2.1.0",
        "micromark-extension-mdxjs": "^3.0.0",
        "micromark-util-types": "^2.0.1",
        "remark": "^15.0.1",
        "safe-ssr": "^0.0.7",
        "satori": "^0.10.14",
        "satori-html": "^0.3.2",
        "surrealdb": "^1.1.0",
        "svelte-material-icons": "^3.0.5",
        "svelte-parse": "^0.1.1",
        "svelte-preprocess-markdown": "^2.7.3",
        "typescript-svelte-plugin": "^0.3.44",
        "vite-plugin-arraybuffer": "0.0.2"
    }
